            },
            "type": "composition"
          },
          "large": {
            "value": {
              "paddingLeft": "{sizing.xlarge}",
              "paddingRight": "{sizing.xlarge}",
              "itemSpacing": "{sizing.medium}"
            },
            "type": "composition"
          }
        }
      },
      "sizing": {
        "small": {
          "value": "$sizing.base / 1.2",
          "type": "sizing"
        },
        "xmedium": {
          "value": "$sizing.small * 2",
          "type": "sizing"
        },
        "xsmall": {
          "value": "$sizing.base / 1.5",
          "type": "sizing"
        },
        "xxsmall": {
          "value": "$sizing.base / 3",
          "type": "sizing"
        },
        "infographic": {
