          "type": "sizing"
        },
        "base": {
          "value": "24",
          "type": "sizing"
        },
        "medium": {
          "value": "$sizing.base * 1.5 - $sizing.xxxsmall",
          "type": "sizing"
        },
        "xxmedium": {
          "value": "$sizing.base * 2",
          "type": "sizing"
        },
        "large": {
          "value": "$sizing.base * 2  + {sizing.xsmall}",
          "type": "sizing"
        },
        "mid-large": {
          "value": "$sizing.base * 3",
          "type": "sizing"
        },
        "xlarge": {
          "value": "$sizing.base * 4",
          "type": "sizing"
        }
